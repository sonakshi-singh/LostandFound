<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>issuePass</servlet-name>
        <servlet-class>Servlet.IssuePassServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getpendingReqServlet</servlet-name>
        <servlet-class>Servlet.getpendingReqServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ISServlet</servlet-name>
        <servlet-class>Servlet.ISServlet</servlet-class>
        <init-param>
            <param-name>Name</param-name>
            <param-value>Value</param-value>
        </init-param>
    </servlet>
    <servlet>
        <servlet-name>APRServlet</servlet-name>
        <servlet-class>Servlet.APRServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ApproveReqServlet</servlet-name>
        <servlet-class>Servlet.ApprovedReqServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ApprovedReqServlet</servlet-name>
        <servlet-class>Servlet.ApprovedReqServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ARSBServlet</servlet-name>
        <servlet-class>Servlet.AdminCanceledReqServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userServlet</servlet-name>
        <servlet-class>Servlet.userServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CollectedServlet</servlet-name>
        <servlet-class>Servlet.CollectedServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MatchedServlet</servlet-name>
        <servlet-class>Servlet.MatchedServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HistoryServlet</servlet-name>
        <servlet-class>Servlet.HistoryServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>issuePass</servlet-name>
        <url-pattern>/issuePass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getpendingReqServlet</servlet-name>
        <url-pattern>/getpendingReqServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ISServlet</servlet-name>
        <url-pattern>/ISServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>APRServlet</servlet-name>
        <url-pattern>/APRServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ApproveReqServlet</servlet-name>
        <url-pattern>/ApproveReqServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ApprovedReqServlet</servlet-name>
        <url-pattern>/ApprovedReqServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ARSBServlet</servlet-name>
        <url-pattern>/ARSBServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userServlet</servlet-name>
        <url-pattern>/userServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CollectedServlet</servlet-name>
        <url-pattern>/CollectedServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MatchedServlet</servlet-name>
        <url-pattern>/MatchedServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HistoryServlet</servlet-name>
        <url-pattern>/HistoryServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
    <resource-ref>
        <res-ref-name>jdbc/LFMS</res-ref-name>
        <res-type>javax.sql.DataSource</res-type>
        <res-auth>Container</res-auth>
        <res-sharing-scope>Shareable</res-sharing-scope>
    </resource-ref>
</web-app>
